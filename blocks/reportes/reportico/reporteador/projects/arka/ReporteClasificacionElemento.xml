<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Reporte Clasificacion Elemento</ReportTitle><ReportDescription>Reporte para la clasificación de elementos</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT placa,  grupo_cuentasalida as cuenta_destino,tipo_cambio_bien.id_cuenta_anterior as cuenta_origen,concat(grupo_cuentasalida,tipo_cambio_bien.id_cuenta_anterior) cuentas,salida.fecha_registro as fecha_salida,tipo_cambio_bien.date_cambio fecha_movimiento, (elemento.total_iva_con - coalesce(elemento.ajuste,0) + ajuste_inflacionario) as total_valor_ajustado,
elemento.descripcion as descripcion,&quot;ESF_DEP_ENCARGADA&quot; as dependencia, &quot;FUN_NOMBRE&quot; as funcionario,ValorLibroPrueba(salida.fecha_registro,grupo_vidautil,elemento.total_iva_con - coalesce(elemento.ajuste,0),0,ajuste_inflacionario,2) total_depreciacion,
ValorLibroPrueba(salida.fecha_registro,grupo_vidautil,elemento.total_iva_con - coalesce(elemento.ajuste,0),0,ajuste_inflacionario,3) valor_libros
 FROM arka_inventarios.elemento_individual
 JOIN arka_inventarios.elemento ON elemento.id_elemento=elemento_individual.id_elemento_gen  
  JOIN arka_inventarios.entrada ON entrada.id_entrada=elemento.id_entrada
  JOIN arka_inventarios.salida ON salida.id_salida=elemento_individual.id_salida  
  JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=nivel
  JOIN arka_inventarios.tipo_cambio_bien ON arka_inventarios.tipo_cambio_bien.id_elemento=elemento_individual.id_elemento_gen
JOIN arka_parametros.arka_funcionarios ON arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;=elemento_individual.funcionario
  JOIN arka_parametros.arka_dependencia  ON arka_parametros.arka_dependencia.&quot;ESF_ID_ESPACIO&quot;=arka_inventarios.elemento_individual.ubicacion_elemento
  JOIN catalogo.catalogo_lista ON catalogo.catalogo_elemento.elemento_catalogo=catalogo.catalogo_lista.lista_id 
   JOIN grupo.catalogo_elemento ON cast(grupo.catalogo_elemento.elemento_id as character varying)=catalogo.catalogo_elemento.elemento_grupoc 
    JOIN grupo.grupo_descripcion ON grupo.grupo_descripcion.grupo_id=cast(grupo.catalogo_elemento.elemento_id as character varying)
   
    WHERE catalogo.catalogo_elemento.elemento_id&gt;0 
     AND catalogo.catalogo_lista.lista_activo=1  
      AND elemento.tipo_bien &lt;&gt; 1
       AND entrada.estado_entrada &lt;&gt; 3
        AND elemento.estado=TRUE 
        AND tipo_cambio_bien.estado='Activo';</SQLRaw></SQL><QueryColumns><QueryColumn><Name>placa</Name><TableName></TableName><ColumnName>placa</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>cuenta_destino</Name><TableName></TableName><ColumnName>grupo_cuentasalida as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>cuenta_origen</Name><TableName></TableName><ColumnName>tipo_cambio_bien.id_cuenta_anterior as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>cuentas</Name><TableName></Ta